<p id="notice"><%= notice %></p>

<h1>Project Sponsors</h1>

<table>
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Project Description</th>
      <th>Project Sponsor's Name</th>
      <th>Project Sponsor's Webpage Link</th>
      <th>Project Scope</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_sponsors.each do |project_sponsor| %>
      <tr>
        <td><%= project_sponsor.ProjectName %></td>
        <td><%= project_sponsor.ProjectDesc %></td>
        <td><%= project_sponsor.ProjectSponsorName %></td>
        <td><%= project_sponsor.ProjectSponsorLink %></td>
        <td><%= project_sponsor.ProjectScope %></td>
        <td><%= link_to 'Show', project_sponsor %></td>
        <td><%= link_to 'Edit', edit_project_sponsor_path(project_sponsor) %></td>
        <td><%= link_to 'Destroy', project_sponsor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project Sponsor', new_project_sponsor_path %>
